{
    "name": "Better Roblox",
    "version": "0.0.1",
    "manifest_version": 2,
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "chrome_url_overrides": {
        "roblox.com": "./dist/index.html"
    },
    "offline_enabled": true,
    "permissions": ["tabs"]
}